page {
  background-color: white;
  width: 100%;
  height: 100%;
  margin: 0%;
  padding: 0%;
}

.ContenedorPrincipal {
  width: 100%;
  height: 100%;
  display: flex;
  flex-flow: column;
  border-radius: 16px;
}

.mapa1 {
  width: 100%;
  background-color: aquamarine;
  height: 100%;
  position: absolute;
  top: 0%;
  /* z-index: -1; */
}

.topbar {
  position: absolute;
  top: 0%;
  /* margin-top: 1%; */
  padding-bottom: 2%;
  width: 100%;
  height: 7%;
  /* margin: 0%; */
  display: flex;
  flex-flow: row;
  background: rgba(255, 255, 255, 1.0);
  background: -webkit-linear-gradient(top, rgba(255, 255, 255, 1.0), rgba(255, 255, 255, 0.3));
  background: -moz-linear-gradient(top, rgba(255, 255, 255, 1.0), rgba(255, 255, 255, 0.3));
  background: linear-gradient(to bottom, rgba(255, 255, 255, 1.0), rgba(255, 255, 255, 0.3));
  z-index: 2;
}

