.info {
  width: 90%;
  height: 20%;
  border-radius: 10px;
  margin-left: auto;
  margin-right: auto;
  display: flex;
  flex-flow: column;
  background: #5B10BA;
}

.SeleccionText {
  width: 264px;
  height: 28px;
  font-family: 'Poppins', sans-serif;
  font-style: normal;
  font-weight: normal;
  font-size: 17px;
  line-height: 28px;
  margin-left: auto;
  margin-right: auto;
  margin-top: 3%;
  color: #FFFFFF;
}

.fraseText {
  width: 112px;
  height: 14px;
  font-family: 'Poppins';
  font-style: normal;
  font-weight: normal;
  font-size: 14px;
  line-height: 14px;
  margin-left: auto;
  margin-right: auto;
  margin-top: 1%;
  color: #FFFFFF;
}

.destinopartida {
  width: 90%;
  height: 50%;
  background: #FFFFFF;
  border-radius: 20px;
  margin-top: 5%;
  margin-left: auto;
  margin-right: auto;
  display: flex;
  flex-flow: column;
}

.puntoPartida {
  display: flex;
  flex-flow: row;
  width: 90%;
  height: 45%;
  margin-left: auto;
  margin-right: auto;
  margin-top: 1%;
}

.imageiconpartida {
  width: 10%;
  height: 95%;
  margin-left: 5%;
  border-radius: 15px;
}

.textpuntopartida {
  margin-top: auto;
  margin-bottom: auto;
  margin-left: 10%;
  width: 118px;
  height: 14px;
  font-family: "Poppins";
  font-style: normal;
  font-weight: normal;
  font-size: 14px;
  line-height: 14px;
  color: #000000;
}

.destino {
  display: flex;
  flex-flow: row;
  margin-top: 1%;
  margin-left: auto;
  margin-right: auto;
  height: 45%;
  width: 90%;
}

.imageicondestino {
  width: 10%;
  height: 95%;
  margin-left: 5%;
  border-radius: 15px;
}

.boton {
  width: 90%;
  height: 15%;
  margin-left: auto;
  margin-right: auto;
  margin-top: 5%;
  display: flex;
  flex-flow: row;
}

.botonConfirmar {
  background: #5B10BA;
  border-radius: 10px;
  height: 100%;
  width: 100%;
  display: flex;
  flex-flow: row;
  margin: auto;
}

.botonConfirmar text {
  font-family: 'Poppins';
  font-style: normal;
  font-weight: normal;
  font-size: 17px;
  line-height: 15px;
  margin-top: auto;
  margin-bottom: auto;
  margin-left: 40%;
  margin-right: auto;
}

.botonConfirmar image {
  margin-right: 3%;
  width: 12%;
  height: 80%;
  margin-top: auto;
  margin-bottom: auto;
}